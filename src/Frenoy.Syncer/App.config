  </configSections>
  <connectionStrings>
    <!-- SYNOLOGY -->
    <add name="ttc" providerName="MySql.Data.MySqlClient" connectionString="server=pongit;UserId=root;Password=my-secret-pw;database=ttc_erembodegem;Port=33060;CharSet=utf8;Persist Security Info=True;Convert Zero Datetime=True"/>

    <!-- VAGRANT -->
